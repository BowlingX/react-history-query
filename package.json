{
  "name": "react-history-query",
  "version": "0.0.0-development",
  "description": "a higher order component to connect the history with component props",
  "main": "dist/commonjs/index.js",
  "module": "dist/es6/index.js",
  "scripts": {
    "lint": "flow check && eslint src/",
    "start": "styleguidist server",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "cross-env BABEL_ENV=test karma start --single-run --ci --test",
    "tdd": "cross-env BABEL_ENV=test karma start --auto-watch --test",
    "build:es": "cross-env BABEL_ENV=es babel src/lib --out-dir dist/es6",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src/lib --out-dir dist/commonjs",
    "cleanProject": "rimraf dist test_out",
    "prepublishOnly": "npm run cleanProject && npm run build:es && npm run build:commonjs",
    "semantic-release": "semantic-release"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BowlingX/react-history-query.git"
  },
  "release": {
    "debug": false
  },
  "keywords": [
    "react",
    "url",
    "query",
    "query parameter"
  ],
  "author": "David Heidrich <me@bowlingx.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BowlingX/react-history-query/issues"
  },
  "homepage": "https://github.com/BowlingX/react-history-query#readme",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "history": "^4.7.2",
    "query-string": "^4.3.4",
    "re-reselect": "^0.5.0",
    "reselect": "^3.0.1",
    "shallowequal": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-export-extensions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.85.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-styleguidist": "^6.1.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^15.1.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0"
  }
}
